{
  "name": "manage",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node ./commit.js"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier --write",
      "vue-cli-service lint",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.26.0",
    "core-js": "^3.6.4",
    "crypto-js": "^4.1.1",
    "echarts": "4.8.0",
    "element-ui": "2.14.1",
    "nprogress": "^0.2.0",
    "register-service-worker": "^1.7.2",
    "sass": "^1.62.1",
    "vue": "2.6.11",
    "vue-i18n": "^8.18.2",
    "vue-meta": "^2.4.0",
    "vue-router": "3.1.6",
    "vuedraggable": "^2.17.0",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.3.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-service": "~4.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-es2015": "^6.24.1",
    "compression-webpack-plugin": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.4.1",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.1.1",
    "vue-template-compiler": "2.6.11",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "plugins": [
      "import"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "import/order": [
        "error",
        {
          "groups": [
            "builtin",
            "external",
            "internal",
            [
              "parent",
              "sibling",
              "index"
            ],
            "object",
            "type"
          ],
          "pathGroups": [
            {
              "pattern": "react*",
              "group": "external"
            },
            {
              "pattern": "@/components/**",
              "group": "sibling"
            }
          ],
          "pathGroupsExcludedImportTypes": [],
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc",
            "caseInsensitive": true
          }
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
